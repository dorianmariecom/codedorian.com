<%= form_for(show_url) do |f| %>
  <div class="p">
    <%= f.label(:submission_id, class: :label) %>
    <%= f.select(
      :submission_id,
      submission_options(submission_id: f.object.submission_id),
      {},
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_program_id, class: :label) %>
    <%= f.select(
      :form_program_id,
      form_program_options(form_program_id: f.object.form_program_id),
      {},
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_schedule_id, class: :label) %>
    <%= f.select(
      :form_schedule_id,
      form_schedule_options(form_schedule_id: f.object.form_schedule_id),
      {},
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_delivery_id, class: :label) %>
    <%= f.select(
      :form_delivery_id,
      form_delivery_options(form_delivery_id: f.object.form_delivery_id),
      {},
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:name, class: :label) %>
    <%= f.text_field(
      :name,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:description, class: :label) %>
    <%= f.text_area(
      :description,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:position, class: :label) %>
    <%= f.number_field(
      :position,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_program_name, class: :label) %>
    <%= f.text_field(
      :form_program_name,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_program_description, class: :label) %>
    <%= f.text_area(
      :form_program_description,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_schedule_starts_at, class: :label) %>
    <%= f.datetime_field(
      :form_schedule_starts_at,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_schedule_interval, class: :label) %>
    <%= f.text_field(
      :form_schedule_interval,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_schedule_name, class: :label) %>
    <%= f.text_field(
      :form_schedule_name,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_schedule_description, class: :label) %>
    <%= f.text_area(
      :form_schedule_description,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_delivery_name, class: :label) %>
    <%= f.text_field(
      :form_delivery_name,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.label(:form_delivery_description, class: :label) %>
    <%= f.text_area(
      :form_delivery_description,
      class: :input,
      autocomplete: :off,
      data: {
        form_type: :other
      }
    ) %>
  </div>

  <div class="p">
    <%= f.submit(
      submit,
      class: "button",
      data: {
        controller: "bridge--button",
        bridge__button_title_value: submit
      }
    ) %>
  </div>
<% end %>
