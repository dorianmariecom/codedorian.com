<% @docs.each do |doc| %>
  <details>
    <summary class="p font-bold">
      <%= link_to(doc["name"], doc_path(doc["name"]), class: "link") %>
    </summary>

    <%= render "docs/examples", examples: doc["examples"] %>
    <%= render "docs/functions", type: :class_functions, doc: doc %>
    <%= render "docs/functions", type: :instance_functions, doc: doc %>
  </details>
<% end %>
